</head>
<body>
    <main class="centered">
        <main>
            <div class="row">
                <div class="col-lg-12">
                    <div class="list-group">
                        <!-- loop over blog posts and render them -->
                        <% search.forEach((sear) => { %>
                            <a href="#" class="list-group-item">
                                <h4 class="list-group-item-heading"><%= sear.searchText %></h4>
                                <p class="list-group-item-text"><%= sear._id %></p>
                            </a>
                            <br>
                            <br>
                        <% }) %>
                    </div>
                </div>
            </div>
        </main>
    </main>
</body>
